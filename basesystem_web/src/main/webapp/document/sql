/*菜单表*/
DROP TABLE SYSTEM_MENU;
CREATE TABLE SYSTEM_MENU
(
  NODE_ID      INT  PRIMARY KEY,
  NODE_NAME    VARCHAR(200),
  ICON_CLS     VARCHAR(200),
  URL          VARCHAR(1000),
  NODE_TYPE    VARCHAR(200),
  PARENT_ID    INT,
  ORDER_ID     INT,
  REMARKS      VARCHAR(2000),
  DELETE_FLAG  INT(1)                        DEFAULT 0
)